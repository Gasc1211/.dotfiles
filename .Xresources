! My Default
! ------------------------------------------------------------------------------
! ROFI Color theme
! ------------------------------------------------------------------------------
! Use extended color scheme
rofi.color-enabled:                  true
!! Color scheme for normal row
rofi.color-normal:                   argb:00000000,  #073642, argb:00000000,  #073642,  #976490
!! Color scheme for urgent row
rofi.color-urgent:                   argb:00000000,  #073642, argb:00000000,  #dc322f , #976490
!! Color scheme for active row
rofi.color-active:                   argb:00000000,  #073642, argb:00000000,  #586e75 , #976490
!! Color scheme window
rofi.color-window:                   argb:eeffffff,  #002b36,  #002b36
!! Separator style (none, dash, solid)
rofi.separator-style:                dash
